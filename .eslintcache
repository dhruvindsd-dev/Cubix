artItems;\r\n  if (IsLoading) cartItems = <Loader />;\r\n  else if (props.cart.length > 0)\r\n    // if i dierctly assign props to cartItems staggering will not work\r\n    cartItems = (\r\n      <motion.div variants={Card2StaggerParent}>\r\n        {props.cart.map((item, i) => (\r\n          <motion.div variants={Card2StaggerChild} className=\"this is a test \">\r\n            <Card2\r\n              title={item.title}\r\n              price={item.price}\r\n              discount={item.discount}\r\n              id={item.id}\r\n              img={item.img}\r\n              removeClick={handleRemoveItem.bind(this, item.id)}\r\n              key={i}\r\n            />\r\n          </motion.div>\r\n        ))}\r\n      </motion.div>\r\n    );\r\n  else cartItems = <p>No Items Found In The Cart </p>;\r\n  return cartItems;\r\n};\r\n\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    cart: state.cart.cart,\r\n    hasCartLoaded: state.cart.hasCartLoaded,\r\n    isAuthenticated: state.auth.isAuthenticated,\r\n  };\r\n};\r\nconst mapDispatchToProps = (dispatch) => {\r\n  return {\r\n    removeItem: (id) => {\r\n      dispatch({ type: REMOVE_ITEM, payload: { id: id } });\r\n    },\r\n    loadCart: (cart) => {\r\n      dispatch({ type: LOAD_CART, payload: { cart: cart } });\r\n    },\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(MainCart);\r\n","C:\\development\\cubix\\src\\cubix\\src\\pages\\Cart\\SideBar.js",[],"C:\\development\\cubix\\src\\cubix\\src\\pages\\Orders\\SideBar.js",[],"C:\\development\\cubix\\src\\cubix\\src\\components\\Rating\\Rating.js",[],"C:\\development\\cubix\\src\\cubix\\src\\shared.js",[],"C:\\development\\cubix\\src\\cubix\\src\\Hooks\\Fetch.js",["174"],"C:\\development\\cubix\\src\\cubix\\src\\pages\\Shop\\Grid\\GridList.js",[],"C:\\development\\cubix\\src\\cubix\\src\\pages\\Shop\\Grid\\GridItem.js",[],{"ruleId":"175","severity":1,"message":"176","line":3,"column":10,"nodeType":"177","messageId":"178","e